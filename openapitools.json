{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.3.1",
    "storageDir": "~/my/custom/storage/dir", 
    "generators": { 
      "v3.0": { 
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/client/#{name}",
        "glob": "src/flavorite.yaml"
      }
    }
  }
}
